{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shivanshdata"
		},
		"adlsv2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsv2'"
		},
		"mysql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mysql'"
		},
		"adlsv2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shivanshacc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shivanshdata_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adlsv2_accountKey": {
						"value": "[parameters('adlsv2_accountKey')]"
					},
					"mysql_connectionString": {
						"value": "[parameters('mysql_connectionString')]"
					},
					"adlsv2_properties_typeProperties_url": {
						"value": "[parameters('adlsv2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "shivanshdata_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'shivanshdata_ArmTemplate_0')]"
			]
		}
	]
}